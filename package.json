{
  "name": "builder-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sw:build": "esbuild src/components/sideMenu/propertiesMenu/cspMenu/serviceWorker.ts --bundle --platform=browser --format=iife --outfile=public/serviceWorker.js",
    "sw:watch": "esbuild src/components/sideMenu/propertiesMenu/cspMenu/serviceWorker.ts --bundle --platform=browser --format=iife --outfile=public/serviceWorker.js --watch",
    "nf:build": "esbuild src/components/sideMenu/propertiesMenu/nativeFunctionsMenu/nativeFunctions.ts --bundle --platform=browser --format=iife --outfile=public/nativeFunctions.js",
    "nf:watch": "esbuild src/components/sideMenu/propertiesMenu/nativeFunctionsMenu/nativeFunctions.ts --bundle --platform=browser --format=iife --outfile=public/nativeFunctions.js --watch",
    "dev:3000": "next dev --port 3000 --turbopack",
    "dev:3001": "next dev --port 3001 --turbopack",
    "dev": "concurrently -k \"npm:sw:watch\" \"npm:nf:watch\" \"npm:dev:3000\" \"npm:dev:3001\"",
    "build": "npm run sw:build && npm run nf:build && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "node copy-zone.mjs",
    "prebuild": "node copy-zone.mjs"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-tree-view": "^8.9.2",
    "@react-three/drei": "^10.0.4",
    "@react-three/fiber": "^9.1.0",
    "@tylertech/forge": "^3.10.4",
    "@tylertech/forge-react": "^3.1.0",
    "@ui5/webcomponents-react": "^2.12.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.5.0",
    "lz-string": "^1.5.0",
    "mui-color-input": "^7.0.0",
    "next": "^15.5.4",
    "postcss": "^8.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "three": "^0.174.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.174.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
